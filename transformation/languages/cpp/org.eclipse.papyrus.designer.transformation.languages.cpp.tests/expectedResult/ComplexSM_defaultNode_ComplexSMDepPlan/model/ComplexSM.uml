	activeStateID = STATE1_ID;
	//starting the counters for time events
	setFlag(COMPLEXSM_TE_INDEX(TE_VALUE_500_UNIT_MS__ID), statemachine::TF_TIME_EVENT, true);
	systemState = statemachine::EVENT_CONSUMED;
	}
}
