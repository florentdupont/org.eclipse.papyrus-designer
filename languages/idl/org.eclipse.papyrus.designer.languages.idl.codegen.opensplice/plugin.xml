<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.papyrus.designer.languages.idl.codegen.opensplice.preferences.IDLCodeGenPreferenceInitializer">
      </initializer>
	</extension>
	<extension
         point="org.eclipse.papyrus.designer.languages.common.extensionpoints.languageCodegen">
      <generator
      		id="OpenSplice IDL to C++"
			class="org.eclipse.papyrus.designer.languages.idl.codegen.opensplice.IDLCPPLangCodegen"
            language="IDLC++">
      </generator>
   </extension>
</plugin>
