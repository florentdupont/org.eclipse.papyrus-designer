            class="org.eclipse.papyrus.designer.languages.cpp.codegen.preferences.CppCodeGenPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.papyrus.designer.languages.common.extensionpoints.languageCodegen">
