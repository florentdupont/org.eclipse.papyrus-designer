            <language>C/C++</language>
            <body></body>
          </ownedBehavior>
          <ownedOperation xmi:type="uml:Operation" xmi:id="_NVBmIBbXEeasT4HdO7q9gg" name="run" method="_tyjRwc__Ed-TP80eVS-2Rg"/>
        </packagedElement>
