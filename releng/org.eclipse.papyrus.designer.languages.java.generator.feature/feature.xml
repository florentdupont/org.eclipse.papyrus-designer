   <requires>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.eclipse.uml2.uml"/>
      <import plugin="org.eclipse.m2m.qvt.oml"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.jdt.launching"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.uml2.common" version="1.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.common" version="2.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="org.eclipse.uml2.common.edit"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.emf.edit.ui"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.resources" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <!--Designer-->
      <import plugin="org.eclipse.papyrus.designer.languages.java.generator.metamodel.jdt" version="0.7.0.qualifier" match="greaterOrEqual"/>
      <import plugin="org.eclipse.papyrus.designer.languages.java.generator.transfo.umltojdt" version="0.7.0.qualifier" match="greaterOrEqual"/>
      <import plugin="org.eclipse.papyrus.designer.languages.java.generator.jdtsynchronizer" version="0.7.0.qualifier" match="greaterOrEqual"/>
      <import feature="org.eclipse.papyrus.designer.languages.java.profile.feature" version="0.7.0.qualifier"/>
   </requires>

   <plugin
         id="org.eclipse.papyrus.designer.languages.java.generator.jdtsynchronizer"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.designer.languages.java.generator.metamodel.jdt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.designer.languages.java.generator.metamodel.jdt.edit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.designer.languages.java.generator.metamodel.jdt.editor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.designer.languages.java.generator.transfo.umltojdt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.designer.languages.java.generator.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
